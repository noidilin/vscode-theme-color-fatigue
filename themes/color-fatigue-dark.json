  "semanticHighlighting": true,
  "semanticTokenColors": {
    "comment": { "foreground": "#555555" },
    "string": { "foreground": "#707070" },
    "keyword": { "foreground": "#9d9d9d" },
    "operator": { "foreground": "#9d9d9d" },
    "number": { "foreground": "#878787" },
    "regexp": "#707070",
    // namespace - "entity.name.namespace"
    "namespace": { "foreground": "#eaeaea", "fontStyle": "underline" },
    // interface - "entity.name.type.interface"
    "interface": { "foreground": "#eaeaea" },
    // type - "entity.name.type"
    "type": { "foreground": "#eaeaea" },
    // class - "entity.name.type.class"
    "class": { "foreground": "#878787" },
    // variable - "entity.name.variable", "variable.other.readwrite"
    "variable": { "foreground": "#878787" },
    // property - "variable.other.property"
    "property": { "foreground": "#8e8b85", "fontStyle": "italic" },
    // event - "variable.other.event"
    "event": { "foreground": "#8e8b85", "fontStyle": "underline" },
    // parameter - "variable.parameter"
    "parameter": {
      "foreground": "#878787",
      "fontStyle": "bold underline"
    },
    // function -"entity.name.function"
    "function": { "foreground": "#c0c0c0" },
    "function.async": {
      "foreground": "#c0c0c0",
      "fontStyle": "bold underline"
    },
    // method - "entity.name.function.member"
    "method": { "foreground": "#c0c0c0", "fontStyle": "italic" }
  },

  "tokenColors": [
    // - Keyword
    // keyword - #9d9d9d
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.operator.logical",
        "keyword.operator.constructor",
        // things relating to storing data or info
        "storage"
      ],
      "settings": {
        "foreground": "#9d9d9d"
      }
    },
    // package import - #878787
    {
      "name": "Package Import",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#878787"
      }
    },

    // - Type & Class
    // types - #eaeaea
    {
      "name": "Types",
      "scope": [
        "storage.type",
        // library class/type
        "support.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#eaeaea"
      }
    },
    // class - #878787
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#878787"
      }
    },
    // class inherited - #c0c0c0
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#c0c0c0"
      }
    },

    // - Function
    // function - #c0c0c0
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function",
        // built-in functions
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    // parameter - #878787
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#878787"
      }
    },
    // function call - #8e8b85
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#8e8b85"
      }
    },

    // - Constant & Variable
    // constant - #b4b0a7
    {
      "name": "Constant",
      "scope": [
        "constant.language",
        // library constant
        "constant.character",
        "support.constant"
      ],
      "settings": {
        "foreground": "#b4b0a7"
      }
    },
    // constant deSat - #eaeaea
    {
      "name": "Constant De-saturation",
      "scope": [
        // built-in constant
        "constant",
        // css color
        "constant.other"
      ],
      "settings": {
        "foreground": "#eaeaea"
      }
    },
    // variable - #878787
    {
      "name": "Variable Object",
      "scope": [
        "variable",
        "string variable",
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#878787"
      }
    },
    // variable language - #9d9d9d
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#9d9d9d"
      }
    },
    // variable other - #8e8b85
    {
      "name": "Other variable",
